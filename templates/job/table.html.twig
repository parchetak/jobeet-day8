<table class="table table-striped table-hover text-center">
    <thead>
        <tr>
            <th class="table-active text-center">City</th>
            <th class="table-active text-center">Position</th>
            <th class="table-active text-center">Company</th>
        </tr>
    </thead>

    <tbody>
    {% for job in jobs %}
        <tr>
            <td> {{ job.location }} </td>
            <td> <a href="{{ path('job.show', {id: job.id}) }}"> {{ job.position }} </a> </td>
            <td> {{ job.company }} </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
